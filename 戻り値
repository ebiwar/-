<script>

function バナナ(old){
  if (old < 20){
    document.write ('20才未満は利用できません');
    return;
  }

  if (old > 30){
    document.write('利用料は 2,000円です');
  }else{
    document.write('利用料は 1,800円です');
  }
}

バナナ(10);



</script>



